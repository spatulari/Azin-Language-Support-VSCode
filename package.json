{
  "name": "azin-language-support",
  "displayName": "Azin language support",
  "description": "This is an extension that adds syntax highlighting and icons for my own made language \"Azin\"",
  "publisher": "Alex12-Git200",
  "version": "0.1.3",
  "icon": "azin.png",
  "repository": 
  {
    "type": "git",
    "url": "https://github.com/Alex12-Git200/Azin-Language-Support-VSCode"
  },
  "engines": 
  {
    "vscode": "^1.108.1"
  },
  "categories": 
  [
    "Programming Languages"
  ],
  "contributes": 
  {
    "languages": 
    [
      {
        "id": "azin",
        "aliases": ["Azin"],
        "extensions": [".az"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": 
    [
      {
        "language": "azin",
        "scopeName": "source.azin",
        "path": "./syntaxes/azin.tmLanguage.json"
      }
    ]
  }
}
